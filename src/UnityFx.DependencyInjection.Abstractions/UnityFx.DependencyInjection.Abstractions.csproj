<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net35;net46;netstandard2.0</TargetFrameworks>
		<Description>Dependency injection tools for Unity3d.</Description>
		<Company />
		<Authors>Alexander Bogarsukov</Authors>
		<Copyright>Alexander Bogarsukov</Copyright>
		<PackageProjectUrl>https://github.com/Arvtesh/UnityFx.DependencyInjection</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Arvtesh/UnityFx.DependencyInjection</RepositoryUrl>
		<RepositoryType>Git</RepositoryType>
		<PackageTags>Unity3d;DependencyInjection</PackageTags>
		<PackageLicenseUrl>https://github.com/Arvtesh/UnityFx.DependencyInjection/blob/master/LICENSE.md</PackageLicenseUrl>
		<PackageIconUrl></PackageIconUrl>
		<Version>0.1.0</Version>
		<AssemblyVersion>0.1.0.0</AssemblyVersion>
		<FileVersion>0.1.0.0</FileVersion>
		<InformationalVersion>0.1.0.0</InformationalVersion>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<NeutralLanguage>en-US</NeutralLanguage>
		<RootNamespace>UnityFx.DependencyInjection</RootNamespace>
		<CodeAnalysisRuleSet>../CodingConventions/Cs/CsharpRules.ruleset</CodeAnalysisRuleSet>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="StyleCop.Analyzers" Version="1.1.0-beta008" PrivateAssets="All" />
	</ItemGroup>

	<ItemGroup>
		<AdditionalFiles Include="../CodingConventions/Common/stylecop.json" />
	</ItemGroup>

</Project>
